# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'iOS_messenger' do
  use_frameworks!

  # Pods for iOS_messenger
# Firebase
pod 'Firebase/Core'
pod 'Firebase/Auth'
pod 'Firebase/Database'
pod 'Firebase/Storage'

# Facebook
pod 'FBSDKLoginKit'

# Google Sign In
pod 'GoogleSignIn'

# Chat dependencies
pod 'MessageKit'
pod 'JGProgressHUD'
pod 'RealmSwift'
pod 'SDWebImage'

end
